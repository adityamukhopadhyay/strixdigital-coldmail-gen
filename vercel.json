{
  "version": 2,
  "builds": [
    {
      "src": "api/python/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/generate-email",
      "dest": "/api/python/generate_email"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 